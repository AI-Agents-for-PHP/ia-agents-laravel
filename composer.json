{
    "name": "caiquebispo/ai-agents-laravel",
    "description": "A package to build AI Agents on top of Laravel",
    "keywords": ["laravel", "ai", "chatgpt"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CaiqueBispo\\AiAgentsLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Caique Bispo",
            "email": "caiquebispo0023@gmmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "openai-php/client": ">=0.8.0",
        "guzzlehttp/guzzle": "^7.8",
        "yethee/tiktoken": "^0.5.0",
        "laravel/frameowkr": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CaiqueBispo\\AiAgentsLaravel\\AiAgentsLaravelServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
